    "@types/supertest": "^6.0.3",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@better-auth/expo": "^1.4.10",
    "@neondatabase/serverless": "^1.0.2",
    "better-auth": "^1.4.10",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "sonare": "^0.0.1"
