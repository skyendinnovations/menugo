{
  "name": "backend",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun run --watch index.ts",
    "lint": "eslint \"src/**/*.ts\" && prettier -c \"src/**/*.ts\"",
    "format": "eslint \"src/**/*.ts\" --fix && prettier \"src/**/*.ts\" --write",
    "migrate": "drizzle-kit migrate",
    "migrate:up": "drizzle-kit migrate up",
    "migrate:down": "drizzle-kit migrate down",
    "migrate:status": "drizzle-kit migrate status",
    "generate": "drizzle-kit generate",
    "studio": "drizzle-kit studio",
    "push": "drizzle-kit push"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1"
  }
}
